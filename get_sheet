function init() {
  Tabletop.init( {
    key: 'https://docs.google.com/spreadsheets/d/1GyQfHDOWdFLDIL9AOeMCICQ3F3bc6qpt0Hp1c52bLeo/edit?usp=sharing',
    simpleSheet: true }
  ).then(function(data, tabletop) { 
    console.log(data)
  })
}
window.addEventListener('DOMContentLoaded', init)
ript src='https://cdnjs.cloudflare.com/ajax/libs/tabletop.js/1.5.1/tabletop.min.js'></script>
<script type='text/javascript'>    
  var publicSpreadsheetUrl = 'https://docs.google.com/spreadsheets/d/1GyQfHDOWdFLDIL9AOeMCICQ3F3bc6qpt0Hp1c52bLeo/edit?usp=sharing';

  function init() {
    Tabletop.init( { key: publicSpreadsheetUrl,
                     callback: showInfo,
                     simpleSheet: true } )
  }

  function showInfo(data, tabletop) {
    alert('Successfully processed!')
    console.log(data);
  }

  window.addEventListener('DOMContentLoaded', init)
</script>
